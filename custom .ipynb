    "import itertools\n",
    "\n",
    "n = [3,4,2,4,8]\n",
    "\n",
    "run_max = list(itertools.accumulate(n, lambda x, y: x if x > y else y))\n",
    "\n",
    "print(\"Original List :\", n)\n",
    "print(\"Running Max   :\", run_max)\n",
    "\n"
